<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="186.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.AuthController">
   <children>
      <Button fx:id="buttonSignIn" layoutX="46.0" layoutY="140.0" mnemonicParsing="false" onAction="#signIn" text="Sign In" />
      <Button fx:id="buttonRegister" layoutX="199.0" layoutY="41.0" mnemonicParsing="false" onAction="#register" text="Register" />
      <Label layoutX="46.0" layoutY="77.0" text="Login" />
      <Label layoutX="46.0" layoutY="105.0" text="Password" />
      <TextField fx:id="loginField" layoutX="110.0" layoutY="73.0" />
      <PasswordField fx:id="passwordField" layoutX="110.0" layoutY="101.0" />
      <Label layoutX="47.0" layoutY="45.0" text="Not a user? Register" />
      <Label fx:id="loginResultLabel" layoutX="126.0" layoutY="144.0" />
   </children>
</AnchorPane>
